{
  "name": "polymerx-cli",
  "version": "0.2.2",
  "description": "Unlock the power of Polymer 3, Web Components and modern web tools.",
  "license": "MIT",
  "repository": "PolymerX/polymerx-cli",
  "bugs": {
    "url": "https://github.com/PolymerX/polymerx-cli/issues",
    "email": "info@godev.space"
  },
  "homepage": "https://github.com/PolymerX/polymerx-cli",
  "author": {
    "name": "LasaleFamine",
    "email": "info@godev.space",
    "url": "https://github.com/PolymerX"
  },
  "main": "dist/cli.js",
  "bin": {
    "polymerx": "dist/cli.js"
  },
  "engines": {
    "node": ">=8 <=9"
  },
  "scripts": {
    "dev": "babel-node src/cli.js",
    "prebuild": "rimraf dist",
    "build": "babel src/ -d dist",
    "prepublish": "yarn build",
    "release": "git pull && standard-version",
    "pretest": "yarn build && rimraf test/output",
    "test": "xo && nyc ava --serial",
    "posttest": "rimraf test/output",
    "pretest:travis": "yarn pretest",
    "test:travis": "yarn test && nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "polymer-cli",
    "cli",
    "polymerx-cli",
    "polymerx",
    "polymer3",
    "polymer",
    "webpack3-polymer",
    "polymer-webpack",
    "polymer-postcss",
    "polymer-service-workers",
    "workbox",
    "polymer-workbox"
  ],
  "dependencies": {
    "babel-loader": "7.1.5",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "chalk": "2.4.1",
    "clean-webpack-plugin": "0.1.19",
    "cli-table": "0.3.1",
    "console-clear": "1.1.0",
    "copy-webpack-plugin": "4.5.2",
    "devcert-san": "0.3.3",
    "ejs-loader": "0.3.1",
    "execa": "0.10.0",
    "get-port": "3.2.0",
    "git-user-local": "0.2.0",
    "gittar": "0.1.1",
    "html-webpack-exclude-assets-plugin": "0.0.7",
    "html-webpack-plugin": "3.2.0",
    "inquirer": "6.0.0",
    "ora": "2.1.0",
    "phunctional": "0.2.1",
    "pify": "3.0.0",
    "postcss-loader": "2.1.6",
    "raw-loader": "0.5.1",
    "replace-in-file": "3.4.0",
    "script-ext-html-webpack-plugin": "2.0.1",
    "validate-npm-package-name": "3.0.0",
    "webpack": "4.16.0",
    "webpack-dev-server": "3.1.4",
    "which": "1.3.1",
    "webpack-merge": "4.1.3",
    "workbox-webpack-plugin": "3.4.1",
    "yargs": "12.0.1"
  },
  "devDependencies": {
    "ava": "0.25.0",
    "babel-cli": "6.26.0",
    "codecov": "3.0.4",
    "glob": "7.1.2",
    "nyc": "12.0.2",
    "puppeteer": "1.6.0",
    "rimraf": "2.6.2",
    "standard-version": "4.4.0",
    "uuid": "3.3.2",
    "xo": "0.21.1"
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ]
  },
  "xo": {
    "space": true,
    "rules": {
      "max-len": [
        "error",
        {
          "code": 110,
          "ignoreUrls": true
        }
      ],
      "capitalized-comments": "off"
    }
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
